{
  "name": "production-computer-use-agent",
  "version": "1.0.0",
  "description": "Production-ready computer use agent using Vercel AI SDK and Google Gemini",
  "main": "dist/production_agent.js",
  "types": "dist/production_agent.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node production_agent.ts",
    "start": "node dist/production_agent.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint production_agent.ts --ext .ts",
    "format": "prettier --write production_agent.ts",
    "type-check": "tsc --noEmit",
    "setup": "npm install && npx playwright install",
    "example:web-research": "ts-node examples/web_research.ts",
    "example:form-filling": "ts-node examples/form_filling.ts"
  },
  "keywords": [
    "ai",
    "agent",
    "computer-use",
    "vercel-ai-sdk",
    "gemini",
    "production",
    "safe",
    "automation",
    "web-automation"
  ],
  "author": "MiniMax Agent",
  "license": "MIT",
  "dependencies": {
    "ai": "^6.8.2",
    "zod": "^3.23.8",
    "playwright": "^1.48.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "google-generative-ai": ">=0.8.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "examples/"
  ]
}